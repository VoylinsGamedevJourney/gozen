[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"
window/size/mode.editor=0
window/stretch/mode.editor="disabled"

