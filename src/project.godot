; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GoZen"
config/description="Video editor "
config/version="0.2.3-alpha"
run/main_scene="uid://b0r37lfkj7wl6"
run/print_header=false
config/use_custom_user_dir=true
config/custom_user_dir_name="gozen"
config/auto_accept_quit=false
config/features=PackedStringArray("4.5", "Forward Plus")
boot_splash/image="uid://be26q5shju2ml"
config/icon="res://icon.svg"
boot_splash/minimum_display_time=500

[autoload]

Settings="*res://scripts/autoloads/settings.gd"
FileHandler="*res://scripts/autoloads/file_handler.gd"
TrackHandler="*res://scripts/autoloads/track_handler.gd"
MarkerHandler="*res://scripts/autoloads/marker_handler.gd"
